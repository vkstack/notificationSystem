<div ng-init="ctrl.getSubCols()">
  <div class="row">
    <h4>Collection Level Subscriptions</h4>
  </div>
  <form>
    <div class="col s6">
      <p>
        <input type="checkbox" id="_user" ng-checked="ctrl.collectionSubscribed.users" ng-model="ctrl.colSubForm.users"/>
        <label for="_user">users</label>
      </p>
      <p>
        <input type="checkbox" id="_news" ng-checked="ctrl.collectionSubscribed.news" ng-model="ctrl.colSubForm.news"/>
        <label for="_news">news</label>
      </p>
      <p>
        <input type="checkbox" id="_c1" ng-checked="ctrl.collectionSubscribed.collectionA" ng-model="ctrl.colSubForm.collectionA"/>
        <label for="_c1">collectionA</label>
      </p>
      <p>
        <input type="checkbox" id="_c2" ng-checked="ctrl.collectionSubscribed.collectionB" ng-model="ctrl.colSubForm.collectionB"/>
        <label for="_c2">collectionB</label>
      </p>
    </div>
    <div class="col s6">
      <button class="btn waves-effect" ng-click="ctrl.subscribeCollection()">Submit</button>
    </div>
  </form>
</div>
