<div ng-init="ctrl.getSubCols()">
  <div class="row">
    <h4>Collection Level Subscriptions</h4>
  </div>
  <form>
    <div class="col s6">
      <p>
        <input type="checkbox" id="_user" ng-model="ctrl.collectionSubscribed.users"/>
        <label for="_user">users</label>
      </p>
      <p>
        <input type="checkbox" id="_news" ng-model="ctrl.collectionSubscribed.news"/>
        <label for="_news">news</label>
      </p>
      <p>
        <input type="checkbox" id="_c1" ng-model="ctrl.collectionSubscribed.collectionA"/>
        <label for="_c1">collectionA</label>
      </p>
      <p>
        <input type="checkbox" id="_c2" ng-model="ctrl.collectionSubscribed.collectionB"/>
        <label for="_c2">collectionB</label>
      </p>
    </div>
    <div class="col s6">
      <button class="btn waves-effect" ng-click="ctrl.subscribeCollection()">Submit</button>
    </div>
  </form>
</div>
